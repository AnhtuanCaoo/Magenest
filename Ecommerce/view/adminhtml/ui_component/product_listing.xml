        <column name="start_time" class="Magenest\Ecommerce\Ui\Component\Listing\Column\StartTime" >
            <settings>
                <dateFormat>yyyy-MM-dd hh:mm:ss</dateFormat>
                <timezone>true</timezone>
                <filter>datetimeRange</filter>
                <dataType>date</dataType>
                <label translate="true">Start time</label>
                <dataScope>start_time</dataScope>
            </settings>
        </column>
        <column name="end_time" class="Magenest\Ecommerce\Ui\Component\Listing\Column\EndTime" >
            <settings>
                <filter>datetimeRange</filter>
                <dataType>date</dataType>
                <dateFormat>yyyy-MM-dd hh:mm:ss</dateFormat>
                <label translate="true">End time</label>
                <timezone>true</timezone>
                <dataScope>end_time</dataScope>

            </settings>
